{
	"name": "krulis-martin/quixam",
	"description": "Quixam: Quick examination tool for online testing.",
	"keywords": ["quick", "exam", "test"],
	"type": "project",
	"license": ["MIT"],
	"require": {
		"php": ">= 8.2",
		"apereo/phpcas": "^1.6",
		"contributte/console": "^0.11",
		"contributte/monolog": "^0.6",
		"contributte/translation": "^2.0",
		"firebase/php-jwt": "^7.0",
		"gedmo/doctrine-extensions": "^3.22",
		"latte/latte": "^3.1",
		"nette/application": "^3.2",
		"nette/bootstrap": "^3.2",
		"nette/caching": "^3.4",
		"nette/database": "^3.2",
		"nette/di": "^3.2",
		"nette/forms": "^3.2",
		"nette/http": "^3.3",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.1",
		"nette/schema": "^1.3",
		"nette/security": "^3.2",
		"nette/utils": "^4.1",
		"nettrine/dbal": "^0.9",
		"nettrine/migrations": "^0.10",
		"nettrine/extensions-atlantic18": "^0.6",
		"nettrine/orm": "^0.9",
		"ramsey/uuid-doctrine": "^2.1",
		"tracy/tracy": "^2.11",
		"league/commonmark": "^2.7"
	},
	"require-dev": {
		"nette/tester": "^2.3",
		"symfony/thanks": "^1",
		"phpstan/phpstan": "^2.1",
		"phpstan/phpstan-nette": "^2.0",
		"vrana/adminer": "^5.4"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"allow-plugins": {
			"symfony/thanks": true
		}
	}
}
